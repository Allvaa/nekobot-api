{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-typings",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "eofline": false,
    "quotemark": [true, "double"],
    "ter-indent": [true, 4],
    "ban-types": false,
    "import-name": false,
    "interface-name": false,
    "max-line-length": false,
    "object-shorthand-properties-first": false
  },
  "rulesDirectory": []
}
