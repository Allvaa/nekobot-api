{
    "name": "nekobot-api",
    "version": "1.0.8",
    "description": "NekoBot API wrapper for Node.js",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "tslint -p . && npm run compile",
        "compile": "tsc -p .",
        "pretty": "prettier --write \"src/*\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Allvaa/nekobot-api.git"
    },
    "keywords": [
        "nekobot",
        "api",
        "wrapper",
        "discord"
    ],
    "author": "Allvaa",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Allvaa/nekobot-api/issues"
    },
    "homepage": "https://github.com/Allvaa/nekobot-api#readme",
    "devDependencies": {
        "@types/superagent": "^4.1.3",
        "husky": "^4.2.3",
        "lint-staged": "^10.0.7",
        "prettier": "^1.19.1",
        "tslint": "^5.20.0",
        "tslint-config-airbnb": "^5.11.2",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-typings": "^0.3.1",
        "typescript": "^3.6.4"
    },
    "dependencies": {
        "superagent": "^5.1.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run compile && lint-staged"
        }
    },
    "lint-staged": {
        "src/*": [
            "prettier --write",
            "git add"
        ]
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false
    }
}
