{
  "name": "nekobot-api",
  "version": "1.3.2",
  "description": "NekoBot API wrapper for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "eslint --ignore-path .gitignore \"src/**/*.ts\" && npm run compile",
    "compile": "tsc -p .",
    "pretty": "prettier --write \"src/*\"",
    "doc": "typedoc --ignoreCompilerErrors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Allvaa/nekobot-api.git"
  },
  "keywords": [
    "nekobot",
    "api",
    "wrapper",
    "discord"
  ],
  "author": "Allvaa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Allvaa/nekobot-api/issues"
  },
  "homepage": "https://github.com/Allvaa/nekobot-api#readme",
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.0.5",
    "typedoc": "^0.17.8",
    "typescript": "^3.8.2"
  },
  "dependencies": {},
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false
  }
}
